extends layout.pug 

block content
    .container
        <script src="https://kit.fontawesome.com/0e7e2729b2.js" crossorigin="anonymous"></script>
        div(class="ui three column stackable grid")
            .row
                div(class="five wide column")
                    h1(class="ui centered header" style="font-size:300%;")
                        | Google Technologies
                    <i class="fas fa-vial"></i>
                    a(class="ui link header" href="https://codelabs.developers.google.com/") CodeLabs
                    br
                    <i class="fas fa-flask"></i>
                    a(class="ui link header" href="https://www.qwiklabs.com/") QwikLab
                    br
                    <i class="fab fa-wpexplorer"></i>
                    a(class="ui link header" href="https://cloud.google.com/apis") APIs Explorer
                    br
                    <i class="fas fa-bezier-curve"></i>
                    a(class="ui link header" href="https://developers.google.com/learn/pathways") Pathways
                div(class="five wide column")
                    div(class="ui centered header" style="font-size:300%;")
                        | Club Resources
                        br
                    <i class="fab fa-google"></i>
                    a(class="ui link header" href="https://developers.google.com/community/dsc-solution-challenge") Google DSC Solution Challenge
                    br
                    <i class="fab fa-google-drive"></i>
                    a(class="ui link header" href="https://drive.google.com/drive/u/0/shared-drives") Shared Drive Link
                div(class="five wide column")
                    div(class="ui centered header" style="font-size:300%;").
                        Study Jams
                    <i class="fas fa-robot"></i>
                    a(class="ui link header" href="https://www.youtube.com/watch?v=k7yizsj10Ws&ab_channel=DeveloperStudentClub") Intro to TenserFlow
                    br
                    <i class="fas fa-book-open"></i>    
                    a(class="ui link header" href="https://www.qwiklabs.com/focuses/3473?catalog_rank=%7B%22rank%22%3A3%2C%22num_filters%22%3A0%2C%22has_search%22%3Atrue%7D&parent=catalog&search_id=10773436") Intro to Google APIs
                    br
                    <i class="fas fa-basketball-ball"></i>
                    a(class="ui link header" href="https://www.youtube.com/watch?v=ITnsAOxnRPI&ab_channel=DeveloperStudentClub") Bracketology with Google Machine Learning